.container {
  padding: 16rpx;
}
.list {
  background: #fff;
  border-radius: 12rpx;
  overflow: hidden;
}
.item {
  display: flex;
  justify-content: space-between;
  padding: 24rpx 32rpx;
  border-bottom: 1px solid #f0f0f0;
}
.item:last-child {
  border-bottom: none;
}
.name {
  font-size: 32rpx;
  font-weight: 500;
  color: #333;
}
.meta {
  font-size: 28rpx;
  color: #888;
}
